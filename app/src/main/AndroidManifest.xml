<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yanglei.myapplication">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera2.full" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterFirstPage" />
        <activity android:name=".RegisterForthPage" />
        <activity android:name=".LoginThirdPage" />
        <activity android:name=".RegisterSecondPage" />
        <activity android:name=".RegisterThirdPage" />
        <activity android:name=".LoginFirstPage" />
        <activity android:name=".LoginSecondPage" />
        <activity android:name=".LoginSuccessfully" />
        <activity android:name=".LoginDigitalCard">
        </activity>
    </application>

</manifest>